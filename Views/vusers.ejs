    <!-- start content -->
    <div class="container-fluid">
        <div class="row wrapper-content">
            <div class="col-md-2">
                <h6>API Description</h6>
                <ul class="ul-style">
                    <li>
                        <a href="#register" class="anchor-style"><i class="fas fa-caret-right style-arrow"></i> /user/register</a>
                    </li>
                    <li>
                        <a href="#login" class="anchor-style"><i class="fas fa-caret-right style-arrow"></i> /user/auth</a>
                    </li>
                    <li>
                        <a href="#profile" class="anchor-style"><i class="fas fa-caret-right style-arrow"></i>
                            /user/profile</a>
                    </li>
                    <li>
                        <a href="#user" class="anchor-style"><i class="fas fa-caret-right style-arrow"></i>
                            /user</a>
                    </li>
                </ul>

            </div>
            <div class="col-md-10 over-style">
                <div class="row new-line" id="register">
                    <div class="col-md-12 content-style">
                        <h4>Register User</h4>
                    </div>
                    <div class="col-md-12 content-style2">
                        <p>Description : This url is for create new user</p>
                        <p>URL : https://apiauthv1.herokuapp.com/user/register</p>
                        <p>Method : <a class="method-color-post">POST</a></p>
                        <p>Schema : </p>
                        <code>
                            {<br>
                                username:{type:String, required: true},<br>
                                password: {type:String, required: true},<br>
                                nama: {type:String, required: true},<br>
                                email: {type:String, required: true},<br>
                                phone: {type:String, required: true},<br>
                                avatar: {type:String},<br>
                                role: {type:String, required: true}<br>
                                }
                        </code>
                        <p>Response Success (200) : <a href="/json/user_post200.json" target="_blank">user_post200.json</a></p>
                        <p>Response Failed (400) : <a href="/json/user_post400.json" target="_blank">user_post400.json</a></p>
                        
                    </div>
                </div>
                <div class="row new-line" id="login">
                    <div class="col-md-12 content-style">
                        <h4>Authentication / Login</h4>
                    </div>
                    <div class="col-md-12 content-style2">
                        <p>Description : this url to authentication user</p>
                        <p>URL : https://apiauthv1.herokuapp.com/user/auth</p>
                        <p>Method : <a class="method-color-post">POST</a></p>                        
                        <p>Schema : </p>
                        <code>
                            {<br>
                                username:{type:String, required: true},<br>
                                password: {type:String, required: true}<br>
                            }
                        </code>
                        <p>Response Success (200) : <a href="/json/user_auth200.json"
                                target="_blank">user_auth200.json</a></p>
                        <p>Response Failed (400) : <a href="/json/user_auth400.json"
                                target="_blank">user_auth400.json</a></p>
                    </div>
                </div>

                <div class="row new-line" id="profile">
                    <div class="col-md-12 content-style">
                        <h4>Get Profile by JWT</h4>
                    </div>
                    <div class="col-md-12 content-style2">
                        <p>Description : This url is to retrieve profile information using payload JWT</p>
                        <p>URL : https://apiauthv1.herokuapp.com/user/profile</p>
                        <p>Method : <a class="method-color">GET</a></p>
                        <p>Request Parameters : </p>
                        <div class="row">
                            <div class="col-md-12">
                                <table class="table table-bordered border-primary tstyle">
                                    <thead>
                                        <tr class="text-center">
                                            <th style="width:130px">#</th>
                                            <th>Descriptions</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td>Token</td>
                                            <td>Required, Active</td>
                                        </tr>
                                        <tr>
                                            <td>Role</td>
                                            <td>User</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <p>Response Success (200) : <a href="/json/user_getprofil200.json"
                                target="_blank">user_getprofil200.json</a>
                        </p>
                        <p>Response Failed (400) : <a href="/json/user_getprofil400.json"
                                target="_blank">user_getprofil400.json</a></p>
                    </div>
                </div>

                <div class="row new-line" id="user">
                    <div class="col-md-12 content-style">
                        <h4>Get All User</h4>
                    </div>
                    <div class="col-md-12 content-style2">
                        <p>Description : This url is to retrieve all user</p>
                        <p>URL : https://apiauthv1.herokuapp.com/user</p>
                        <p>Method : <a class="method-color">GET</a></p>
                        <p>Request Parameters : </p>
                        <div class="row">
                            <div class="col-md-12">
                                <table class="table table-bordered border-primary tstyle">
                                    <thead>
                                        <tr class="text-center">
                                            <th style="width:130px">#</th>
                                            <th>Descriptions</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td>Token</td>
                                            <td>Required, Active</td>
                                        </tr>
                                        <tr>
                                            <td>Role</td>
                                            <td>Admin</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <p>Response Success (200) : <a href="/json/user_list200.json"
                                target="_blank">user_list200.json</a>
                        </p>
                        <p>Response Failed (400) : <a href="/json/user_list400.json"
                                target="_blank">user_list400.json</a></p>
                    </div>
                </div>                
            </div>
        </div>
    </div>
