    <!-- start content -->
    <div class="container-fluid">
        <div class="row wrapper-content">
            <div class="col-md-2">
                <h6>API Description</h6>
                <ul class="ul-style">
                    <li>
                        <a href="#house" class="anchor-style"><i class="fas fa-caret-right style-arrow"></i> /house -
                            GET</a>
                    </li>
                    <li>
                        <a href="#housepost" class="anchor-style"><i class="fas fa-caret-right style-arrow"></i> /house
                            -
                            POST</a>
                    </li>
                    <li>
                        <a href="#housedetail" class="anchor-style"><i class="fas fa-caret-right style-arrow"></i>
                            /house/:id/detail</a>
                    </li>
                    <li>
                        <a href="#houseupdate" class="anchor-style"><i class="fas fa-caret-right style-arrow"></i>
                            /house/:id/update</a>
                    </li>
                    <li>
                        <a href="#houseimage" class="anchor-style"><i class="fas fa-caret-right style-arrow"></i>
                            /house/image/:id</a>
                    </li>
                </ul>

            </div>
            <div class="col-md-10 over-style">
                <div class="row new-line" id="house">
                    <div class="col-md-12 content-style">
                        <h4>House list</h4>
                    </div>
                    <div class="col-md-12 content-style2">
                        <p>Description : This url is for retrieving all house</p>
                        <p>URL : https://apiauthv1.herokuapp.com/house</p>
                        <p>Method : <a class="method-color">GET</a></p>
                        <p>Response Success (200) : <a href="/json/house_get200.json" target="_blank">house_get200.json</a>
                        </p>
                        <p>Response Failed (400) : <a href="/json/house_get400.json"
                                target="_blank">house_get400.json</a></p>

                    </div>
                </div>
                <div class="row new-line" id="housepost">
                    <div class="col-md-12 content-style">
                        <h4>Create New House</h4>
                    </div>
                    <div class="col-md-12 content-style2">
                        <p>Description : this url to add new House</p>
                        <p>URL : https://apiauthv1.herokuapp.com/house</p>
                        <p>Method : <a class="method-color-post">POST</a></p>
                        <p>Request Parameters : </p>
                        <div class="row">
                            <div class="col-md-12">
                                <table class="table table-bordered border-primary tstyle">
                                    <thead>
                                        <tr class="text-center">
                                            <th style="width:130px">#</th>
                                            <th>Descriptions</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td>Token</td>
                                            <td>Required, Active</td>
                                        </tr>
                                        <tr>
                                            <td>Role</td>
                                            <td>Admin</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <p>Schema : </p>
                        <code>
                            {<br>
                                tipe: { type: String, required: true },<br>
                                namarumah: { type: String, required: true },<br>
                                lokasi: { type: String, required: true },<br>
                                harga: { type: String, required: true },<br>
                                luas_tanah: { type: String, required: true },<br>
                                luas_bangunan: { type: String, required: true },<br>
                                jumlah_kamar: { type: String, required: true },<br>
                                alamat: { type: String, required: true },<br>
                                listrik: { type: String, required: true },<br>
                                pdam: { type: String, required: true },<br>
                                telepon: { type: String, required: true },<br>
                                legalitas: { type: String, required: true },<br>
                                image: { type: String },<br>
                                blok: { type: String, required: true },<br>
                                status: { type: String, required: true },<br>
                                developer: {type: String, required: true, type: Mongoose.Schema.Types.ObjectId, ref: "tdeveloper"}<br>
                            }
                        </code>
                        <p>Response Success (200) : <a href="/json/house_post200.json"
                                target="_blank">house_post200.json</a></p>
                        <p>Response Failed (400) : <a href="/json/house_post400.json"
                                target="_blank">house_post400.json</a></p>
                    </div>
                </div>

                <div class="row new-line" id="housedetail">
                    <div class="col-md-12 content-style">
                        <h4>Get house details</h4>
                    </div>
                    <div class="col-md-12 content-style2">
                        <p>Description : This url is to retrieve house details per id house</p>
                        <p>URL : https://apiauthv1.herokuapp.com/house/:id/detail</p>
                        <p>Method : <a class="method-color">GET</a></p>
                        <p>Response Success (200) : <a href="/json/house_getdetail200.json"
                                target="_blank">house_getdetail200.json</a>
                        </p>
                        <p>Response Failed (400) : <a href="/json/house_getdetail400.json"
                                target="_blank">house_getdetail400.json</a></p>
                    </div>
                </div>

                <div class="row new-line" id="houseupdate">
                    <div class="col-md-12 content-style">
                        <h4>Update House Details</h4>
                    </div>
                    <div class="col-md-12 content-style2">
                        <p>Description : This url is to update house details from admin</p>
                        <p>URL : https://apiauthv1.herokuapp.com/house/:id/update</p>
                        <p>Method : <a class="method-color-post">POST</a></p>
                        <p>Request Parameters : </p>
                        <div class="row">
                            <div class="col-md-12">
                                <table class="table table-bordered border-primary tstyle">
                                    <thead>
                                        <tr class="text-center">
                                            <th style="width:130px">#</th>
                                            <th>Descriptions</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td>Token</td>
                                            <td>Required, Active</td>
                                        </tr>
                                        <tr>
                                            <td>Role</td>
                                            <td>Admin</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <p>Schema : </p>
                        <code>
                            {<br>
                                tipe: { type: String, required: true },<br>
                                namarumah: { type: String, required: true },<br>
                                lokasi: { type: String, required: true },<br>
                                harga: { type: String, required: true },<br>
                                luas_tanah: { type: String, required: true },<br>
                                luas_bangunan: { type: String, required: true },<br>
                                jumlah_kamar: { type: String, required: true },<br>
                                alamat: { type: String, required: true },<br>
                                listrik: { type: String, required: true },<br>
                                pdam: { type: String, required: true },<br>
                                telepon: { type: String, required: true },<br>
                                legalitas: { type: String, required: true },<br>
                                blok: { type: String, required: true },<br>
                                developer: {type: String, required: true, type: Mongoose.Schema.Types.ObjectId, ref: "tdeveloper"}<br>
                            }
                        </code>
                        <p>Response Success (200) : <a href="/json/house_update200.json"
                                target="_blank">house_update200.json</a>
                        </p>
                        <p>Response Failed (400) : <a href="/json/house_update400.json"
                                target="_blank">house_update400.json</a></p>
                    </div>
                </div>

                <div class="row new-line" id="houseimage">
                    <div class="col-md-12 content-style">
                        <h4>View Image</h4>
                    </div>
                    <div class="col-md-12 content-style2">
                        <p>Description : This url is used to show image</p>
                        <p>URL : https://apiauthv1.herokuapp.com/house/image/:id</p>
                        <p>Method : <a class="method-color">GET</a></p>
                        <p>Request Parameters : </p>
                        <div class="row">
                            <div class="col-md-12">
                                <table class="table table-bordered border-primary tstyle">
                                    <thead>
                                        <tr class="text-center">
                                            <th style="width:130px">#</th>
                                            <th>Descriptions</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td>Token</td>
                                            <td>Required, Active</td>
                                        </tr>
                                        <tr>
                                            <td>Role</td>
                                            <td>Admin / User</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>                        
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- End content -->
